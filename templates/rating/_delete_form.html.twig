<form method="post" action="{{ path('rating_delete', {'rateId': rating.rateId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rating.rateId) }}">
    <button class="btn">Delete</button>
</form>
